   "source": [
    "# Rabbits and Recurrence Relations\n",
    "Recurrence excercise"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "51765773-ccfe-4b24-a719-eca07a7985a2",
   "metadata": {},
   "outputs": [],
   "source": [
    "def rabbits(n, k = 1):\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    if n == 2: \n",
    "        return 1 \n",
    "\n",
    "    return rabbits (n-1, k) + rabbits (n-2, k)*k"
   ]
  },
  {
